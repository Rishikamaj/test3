package com.newt;

import java.util.ArrayList;

public class Practise {

	public static void main(String[] args) {
		int[] c= {2,9,4,7,6};
		int max=0;
		int[] b=new int[7];
	
		
		
		for(int i=0;i<c.length;i++) {
			 b[i++]=Math.abs(c[i]-c[i+1]);
			
		}
		for(int j=0;j<b.length;j++) {
			if(b[j]>max) {
				max=b[j];
			}
			System.out.println(max);
		}
		
	
		
		

	}}
